{
  "name": "nazwa",
  "version": "1.0.9",
  "description": "Projek Nazwa",
  "scripts": {
    "tailwind": "tailwindcss build build/style.css -o statics/css/tailwind.optimized.css",
    "build": "env NODE_ENV=production postcss build/style.css -o statics/css/tailwind.optimized.css",
    "cz": "git-cz",
    "release": "release-it"
  },
  "author": "Deri Herdianto <derryherdianto@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@tailwindcss/custom-forms": "0.2.1",
    "color": "3.1.2",
    "commitizen": "^3.0.0",
    "cssnano": "^5.0.8",
    "cz-conventional-changelog": "^3.0.1",
    "postcss": "^8.3.9",
    "postcss-cli": "^7.1.2",
    "release-it": "^13.7.1",
    "tailwindcss-multi-theme": "^1.0.3"
  },
  "keywords": [
    "Nazwa",
    "Elektronik",
    "Furniture",
    "Kredit"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "autoprefixer": "^9.8.8",
    "npm": "^8.0.0",
    "tailwindcss": "^1.4.6"
  }
}
